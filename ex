#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_010732F0 .scope module, "Example" "Example" 2 1;
 .timescale 0 0;
v01075108_0 .var "clk", 0 0;
v01074B40_0 .var "reset", 0 0;
    .scope S_010732F0;
T_0 ;
    %delay 5, 0;
    %load/v 8, v01075108_0, 1;
    %inv 8, 1;
    %set/v v01075108_0, 8, 1;
    %jmp T_0;
    .thread T_0;
    .scope S_010732F0;
T_1 ;
    %vpi_call 2 6 "$monitor", "Time: %0t - clk = %b, reset = %b", $time, v01075108_0, v01074B40_0;
    %delay 100, 0;
    %set/v v01074B40_0, 1, 1;
    %delay 200, 0;
    %set/v v01074B40_0, 0, 1;
    %delay 300, 0;
    %vpi_call 2 9 "$finish";
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "E:\program\verilog\example.v";
